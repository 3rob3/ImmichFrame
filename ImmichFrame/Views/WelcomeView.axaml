<UserControl x:Class="ImmichFrame.Views.WelcomeView"
			 xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="900"
			 xmlns:vm="using:ImmichFrame.ViewModels"
             x:DataType="vm:WelcomeViewModel">
	<Design.DataContext>
		<vm:WelcomeViewModel />
	</Design.DataContext>
	<DockPanel>
		<Grid HorizontalAlignment="Center" DockPanel.Dock="Bottom">
			<Button Content="Navigate to Settings"
			 HorizontalAlignment="Stretch"
			 Command="{Binding SettingsCommand}"
			 HorizontalContentAlignment="Center"
			 TabIndex="36"/>
		</Grid>
		<ScrollViewer x:Name="scrollViewer" Padding="20">
			<Grid>
				<Label>
					Welcome to Avalonia!
				</Label>
			</Grid>
		</ScrollViewer>
	</DockPanel>
</UserControl>
